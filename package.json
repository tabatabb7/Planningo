    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.15.1",
    "helmet": "^4.2.0",
    "history": "^4.9.0",
    "jodit-react": "^1.0.66",
    "jwks-rsa": "^1.11.0",
